{
  "name": "finch-site",
  "version": "1.0.0",
  "description": "finch site",
  "main": "index.js",
  "author": "msm",
  "license": "ISC",
  "sideEffects": false,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "cross-env NODE_ENV=production next build && tsc --project tsconfig.server.json",
    "start": "yarn build && cross-env NODE_ENV=production node dist/index.js",
    "export": "next export",
    "type-check": "tsc",
    "lint": "eslint src",
    "lint:write": "eslint src --cache --fix",
    "gql:generate": "graphql-codegen --config codegen.yml",
    "gql:watch": "yarn generate --watch",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.js"
    }
  },
  "lint-staged": {
    "src/**/*.{j,t}s?(x)": [
      "prettier --write",
      "eslint src --cache --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@babel/runtime": "^7.14.0",
    "@hookform/resolvers": "^2.8.5",
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^0.26.0",
    "core-js": "3.21.1",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "framer-motion": "^6.2.8",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "next": "^12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "next-redux-wrapper": "^7.0.5",
    "nookies": "^2.5.2",
    "normalize.css": "^8.0.1",
    "polished": "^4.1.4",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-redux": "^7.2.6",
    "react-slick": "^0.28.1",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5",
    "scroll-into-view-if-needed": "^2.2.29",
    "sharp": "^0.28.2",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^5.3.3",
    "styled-map": "^3.3.0",
    "ua-parser-js": "^0.7.28",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-do-expressions": "^7.14.0",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.2",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/near-operation-file-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.0.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-slick": "^0.23.4",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@types/styled-components": "^5.1.9",
    "@types/ua-parser-js": "^0.7.36",
    "@types/video.js": "^7.3.28",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-plugin-styled-components": "^1.13.1",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.3.0",
    "cz-customizable-ghooks": "^2.0.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "8.0.0",
    "lint-staged": ">=10",
    "nodemon": "^2.0.7",
    "prettier": "^2.5.1",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2",
    "url-loader": "^4.1.1",
    "webp-loader": "^0.6.0"
  }
}
